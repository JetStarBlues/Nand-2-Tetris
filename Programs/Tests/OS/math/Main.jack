class Main {

	function void main () {

		// tecsTest();
		customTest();
	}

	function void customTest () {

		// Verify greater and less than comparisons

		GFX.printBool( 0 > - 32767 );          GFX.println();  // true

		GFX.printBool( 0 > ( - 32767 - 1 ) );  GFX.println();  // true
		GFX.printBool( 0 > (   32767 + 1 ) );  GFX.println();  // true

		GFX.printBool( 0 > ( - 32767 + 1 ) );  GFX.println();  // true
		GFX.printBool( 0 > (   32767 + 2 ) );  GFX.println();  // true

		GFX.printBool( 0 > (   32767 + 3 ) );  GFX.println();  // true

		GFX.printChar( ' ' ); GFX.println();

		GFX.printBool( 20000 > - 30000 );      GFX.println();  // true
	}

	function void tecsTest () {

		var Array r;          // stores test results;

		let r = 8000;

		let r[0] = 2 * 3;                  // 6
		let r[1] = r[0] * (-30);           // 6 * (-30) = -180, (65356)
		let r[2] = r[1] * 100;             // (-180) * 100 = -18000, (47536)
		let r[3] = 1 * r[2];               // 1 * (-18000) = -18000, (47536)
		let r[4] = r[3] * 0;               // 0

		let r[5] = 9 / 3;                  // 3
		let r[6] = (-18000) / 6;           // -3000, (62536)
		let r[7] = 32766 / (-32767);       // 0

		let r[8] = Math.sqrt(9);           // 3
		let r[9] = Math.sqrt(32767);       // 181

		let r[10] = Math.min(345, 123);    // 123
		let r[11] = Math.max(123, -345);   // 123
		let r[12] = Math.abs(27);          // 27
		let r[13] = Math.abs(-32767);      // 32767

		let r[14] = Math.mod(9, 2);        // 1
		let r[15] = Math.mod(99, 21);      // 15


		// Debug
		GFX.printInt( r[ 0] ); GFX.println();
		GFX.printInt( r[ 1] ); GFX.println();
		GFX.printInt( r[ 2] ); GFX.println();
		GFX.printInt( r[ 3] ); GFX.println();
		GFX.printInt( r[ 4] ); GFX.println();
		GFX.printInt( r[ 5] ); GFX.println();
		GFX.printInt( r[ 6] ); GFX.println();
		GFX.printInt( r[ 7] ); GFX.println();
		GFX.printInt( r[ 8] ); GFX.println();
		GFX.printInt( r[ 9] ); GFX.println();
		GFX.printInt( r[10] ); GFX.println();
		GFX.printInt( r[11] ); GFX.println();
		GFX.printInt( r[12] ); GFX.println();
		GFX.printInt( r[13] ); GFX.println();
		GFX.printInt( r[14] ); GFX.println();
		GFX.printInt( r[15] ); GFX.println();

		return;
	}

}